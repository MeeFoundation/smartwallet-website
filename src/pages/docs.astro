---
import { Header, Breadcrumbs, DocsComponent} from "mee-components";
import { header, meeFoundation } from "../config";
import Layout from "../layouts/Layout.astro";

const data = {
  users: {
    enabled: true,
    link: "/docs/users/smartwallet",
    description: "Click below to learn more about our Smartwallet Project"
  },
  developers: {
    enabled: true,
    link: "/docs/developers/smartwallet",
    description: "Our development work on Smartwallet Project is in progress. Click below to learn more and join us if you are interested in developing the new future for the internet"
  },
};
---

<Layout title="Docs">
  <main class="">
    <Header header={header} activePage="docs" location="Mee Smartwallet" />
    <article
      class="relative flex h-full min-h-screen w-screen flex-col items-center gap-6 overflow-hidden"
    >
      <div class="mt-27 pt-16">
        <Breadcrumbs
          className="hidden md:flex"
          breadcrumbs={[
            { title: "Projects", link: `${meeFoundation}projects/` },
            { title: "Smartwallet", link: `/` },
            { title: "Docs" },
          ]}
        />
      </div>
      <DocsComponent data={data} title="Smartwallet Docs" />
    </article>
  </main>
</Layout>
